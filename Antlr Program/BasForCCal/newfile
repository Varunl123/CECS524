PROGRAM BTestSub
INT	A, B, C

CALL SUB1
CALL SUB2(A
CALL SUB3(A,B,C)
END BTestSub.

PROCEDURE SUB1
BEGIN
	//empty subroutine
END SUB1

PROCEDURE SUB2(X)
	INT X
	FLOAT Y
BEGIN
	Y = 1.0
	IF(X>=0)THEN
       	Y = 2.0
	ENDIF
END SUB2
PROCEDURE SUB3(D,E,F)
	INT D,E
	FLOAT F,G
BEGIN
	IF(D*E>7)THEN
     	WHILE(D<100) DO
		D=D+1
		G=G+D
	ENDDO
	ENDIF
END SUB3


